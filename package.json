{
  "name": "urlshortener",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "scripts": {
    "dev": "concurrently \"yarn dev:api\" \"yarn dev:web\" --names \"API,WEB\" --prefix-colors \"blue,green\"",
    "dev:api": "cd UrlShortener.API && dotnet run",
    "dev:web": "cd UrlShortener.Web && yarn dev",
    "build": "yarn build:api && yarn build:web",
    "build:api": "dotnet build UrlShortener.API",
    "build:web": "cd UrlShortener.Web && yarn build",
    "test": "dotnet test UrlShortener.Tests",
    "docker": "docker-compose up -d"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
